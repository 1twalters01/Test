{
    "plugins": [],
    "recurseDepth": 10,
    "compilerOptions": {
      "strict": true,
      "checkJs": true,
    }
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc.tags", "closure"],
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
    }
}
