{
  "plugins": [],
  "recurseDepth": 10,
  "compilerOptions": {
      "strict": true,
      "checkJs": true,
      "jsx": "preserve",
      "jsxImportSource": "solid-js",
  }
  "source": {
      "includePattern": ".+//.js(doc|x|css)?$",
      "excludePattern": "(^|\\/|\\\\)_",
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc.tags", "closure"],
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
  }
}

